<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="t_alreadybuy_goods" catalog="shopping"></table>
	<table name="t_user" catalog="shopping">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_goods" catalog="shopping">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_type" catalog="shopping">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_shoppingcart" catalog="shopping">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="t_seller" catalog="shopping">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>